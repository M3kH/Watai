{
	"timeout"			: 5000,
	"quit"				: "always",
	"views"				: ["CLI", "Growl"],

	"log": {
		"init": {
			"console": {
				"level"		: "error",
				"colorize"	: true
			}
		},
		"load": {
			"console": {
				"level"		: "warn",
				"colorize"	: true
			}
		}
	},
	"matchTriesDelay"	: 100,
	"seleniumServerURL"	: "http://127.0.0.1:4444/wd/hub",
	"debug": {
		"asyncTraces": 40
	}
}
